<?xml version="1.0" ?>
<!--
=============================================================================
WAREHOUSE CBF DEMONSTRATION - INTERSECTION SCENARIO
=============================================================================

This world creates a clear "intersection" scenario where two robots MUST
cross through the same point, guaranteeing CBF collision avoidance activation.

ROBOT PATHS:
    Robot 1 (Blue):  (0, 4) → (0, -4)   Traveling SOUTH (straight down)
    Robot 2 (Green): (-4, 0) → (4, 0)   Traveling EAST (straight right)
    
    Both paths cross at the ORIGIN (0, 0) - collision is guaranteed without CBF!

MARKERS:
    - Blue start marker at (0, 4)
    - Blue goal marker at (0, -4)
    - Green start marker at (-4, 0)
    - Green goal marker at (4, 0)

OBSTACLES:
    - Storage racks positioned to the sides, NOT blocking the intersection
=============================================================================
-->
<sdf version="1.8">
  <world name="warehouse_world">
    
    <!-- ================================================================= -->
    <!-- PHYSICS CONFIGURATION                                              -->
    <!-- ================================================================= -->
    <physics name="1ms" type="ignored">
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1.0</real_time_factor>
    </physics>
    
    <!-- Required Gazebo Plugins -->
    <plugin filename="gz-sim-physics-system" name="gz::sim::systems::Physics"/>
    <plugin filename="gz-sim-scene-broadcaster-system" name="gz::sim::systems::SceneBroadcaster"/>
    <plugin filename="gz-sim-user-commands-system" name="gz::sim::systems::UserCommands"/>
    
    <!-- ================================================================= -->
    <!-- LIGHTING                                                           -->
    <!-- ================================================================= -->
    <light type="directional" name="warehouse_light">
      <cast_shadows>true</cast_shadows>
      <pose>0 0 15 0 0 0</pose>
      <diffuse>0.9 0.9 0.9 1</diffuse>
      <specular>0.3 0.3 0.3 1</specular>
      <attenuation>
        <range>100</range>
        <constant>0.9</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
      <direction>-0.3 0.2 -0.9</direction>
    </light>
    
    <light type="directional" name="fill_light">
      <cast_shadows>false</cast_shadows>
      <pose>0 0 10 0 0 0</pose>
      <diffuse>0.4 0.4 0.5 1</diffuse>
      <specular>0.1 0.1 0.1 1</specular>
      <direction>0.5 -0.3 -0.8</direction>
    </light>
    
    <!-- ================================================================= -->
    <!-- WAREHOUSE FLOOR                                                    -->
    <!-- ================================================================= -->
    <model name="warehouse_floor">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>20 20</size>
            </plane>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>20 20</size>
            </plane>
          </geometry>
          <material>
            <ambient>0.4 0.4 0.4 1</ambient>
            <diffuse>0.5 0.5 0.5 1</diffuse>
            <specular>0.1 0.1 0.1 1</specular>
          </material>
        </visual>
      </link>
    </model>

    <!-- ================================================================= -->
    <!-- INTERSECTION MARKER (Yellow X at origin)                           -->
    <!-- ================================================================= -->
    <!-- This marks where the paths cross - the "danger zone" -->
    
    <model name="intersection_marker">
      <static>true</static>
      <pose>0 0 0.01 0 0 0</pose>
      <link name="link">
        <visual name="circle">
          <geometry>
            <cylinder>
              <radius>0.5</radius>
              <length>0.02</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.0 1</ambient>
            <diffuse>1.0 1.0 0.0 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- ================================================================= -->
    <!-- STORAGE RACK 1 (Upper Left - out of the way)                       -->
    <!-- ================================================================= -->
    <model name="storage_rack_1">
      <static>true</static>
      <pose>-3 3 0 0 0 0</pose>
      
      <link name="rack_body">
        <visual name="left_support">
          <pose>-0.4 0 0.75 0 0 0</pose>
          <geometry><box><size>0.08 0.6 1.5</size></box></geometry>
          <material><ambient>0.3 0.2 0.1 1</ambient><diffuse>0.5 0.35 0.2 1</diffuse></material>
        </visual>
        <collision name="left_support_col">
          <pose>-0.4 0 0.75 0 0 0</pose>
          <geometry><box><size>0.08 0.6 1.5</size></box></geometry>
        </collision>
        
        <visual name="right_support">
          <pose>0.4 0 0.75 0 0 0</pose>
          <geometry><box><size>0.08 0.6 1.5</size></box></geometry>
          <material><ambient>0.3 0.2 0.1 1</ambient><diffuse>0.5 0.35 0.2 1</diffuse></material>
        </visual>
        <collision name="right_support_col">
          <pose>0.4 0 0.75 0 0 0</pose>
          <geometry><box><size>0.08 0.6 1.5</size></box></geometry>
        </collision>
        
        <visual name="bottom_shelf">
          <pose>0 0 0.3 0 0 0</pose>
          <geometry><box><size>0.9 0.5 0.05</size></box></geometry>
          <material><ambient>0.4 0.3 0.2 1</ambient><diffuse>0.6 0.45 0.3 1</diffuse></material>
        </visual>
        
        <visual name="middle_shelf">
          <pose>0 0 0.8 0 0 0</pose>
          <geometry><box><size>0.9 0.5 0.05</size></box></geometry>
          <material><ambient>0.4 0.3 0.2 1</ambient><diffuse>0.6 0.45 0.3 1</diffuse></material>
        </visual>
        
        <visual name="top_shelf">
          <pose>0 0 1.3 0 0 0</pose>
          <geometry><box><size>0.9 0.5 0.05</size></box></geometry>
          <material><ambient>0.4 0.3 0.2 1</ambient><diffuse>0.6 0.45 0.3 1</diffuse></material>
        </visual>
        
        <visual name="box1">
          <pose>-0.15 0 0.45 0 0 0</pose>
          <geometry><box><size>0.25 0.3 0.25</size></box></geometry>
          <material><ambient>0.6 0.4 0.2 1</ambient><diffuse>0.8 0.6 0.4 1</diffuse></material>
        </visual>
        
        <visual name="box2">
          <pose>0.2 0 0.95 0 0 0.1</pose>
          <geometry><box><size>0.2 0.25 0.2</size></box></geometry>
          <material><ambient>0.5 0.3 0.1 1</ambient><diffuse>0.7 0.5 0.3 1</diffuse></material>
        </visual>
      </link>
    </model>
    
    <!-- ================================================================= -->
    <!-- STORAGE RACK 2 (Lower Right - out of the way)                      -->
    <!-- ================================================================= -->
    <model name="storage_rack_2">
      <static>true</static>
      <pose>3 -3 0 0 0 0</pose>
      
      <link name="rack_body">
        <visual name="left_support">
          <pose>-0.4 0 0.75 0 0 0</pose>
          <geometry><box><size>0.08 0.6 1.5</size></box></geometry>
          <material><ambient>0.3 0.2 0.1 1</ambient><diffuse>0.5 0.35 0.2 1</diffuse></material>
        </visual>
        <collision name="left_support_col">
          <pose>-0.4 0 0.75 0 0 0</pose>
          <geometry><box><size>0.08 0.6 1.5</size></box></geometry>
        </collision>
        
        <visual name="right_support">
          <pose>0.4 0 0.75 0 0 0</pose>
          <geometry><box><size>0.08 0.6 1.5</size></box></geometry>
          <material><ambient>0.3 0.2 0.1 1</ambient><diffuse>0.5 0.35 0.2 1</diffuse></material>
        </visual>
        <collision name="right_support_col">
          <pose>0.4 0 0.75 0 0 0</pose>
          <geometry><box><size>0.08 0.6 1.5</size></box></geometry>
        </collision>
        
        <visual name="bottom_shelf">
          <pose>0 0 0.3 0 0 0</pose>
          <geometry><box><size>0.9 0.5 0.05</size></box></geometry>
          <material><ambient>0.4 0.3 0.2 1</ambient><diffuse>0.6 0.45 0.3 1</diffuse></material>
        </visual>
        
        <visual name="middle_shelf">
          <pose>0 0 0.8 0 0 0</pose>
          <geometry><box><size>0.9 0.5 0.05</size></box></geometry>
          <material><ambient>0.4 0.3 0.2 1</ambient><diffuse>0.6 0.45 0.3 1</diffuse></material>
        </visual>
        
        <visual name="top_shelf">
          <pose>0 0 1.3 0 0 0</pose>
          <geometry><box><size>0.9 0.5 0.05</size></box></geometry>
          <material><ambient>0.4 0.3 0.2 1</ambient><diffuse>0.6 0.45 0.3 1</diffuse></material>
        </visual>
        
        <visual name="box1">
          <pose>0.1 0 0.45 0 0 -0.1</pose>
          <geometry><box><size>0.3 0.25 0.25</size></box></geometry>
          <material><ambient>0.2 0.3 0.5 1</ambient><diffuse>0.4 0.5 0.7 1</diffuse></material>
        </visual>
        
        <visual name="box2">
          <pose>-0.15 0 0.95 0 0 0</pose>
          <geometry><box><size>0.25 0.3 0.2</size></box></geometry>
          <material><ambient>0.5 0.2 0.2 1</ambient><diffuse>0.7 0.4 0.4 1</diffuse></material>
        </visual>
      </link>
    </model>

    <!-- ================================================================= -->
    <!-- ROBOT 1: BLUE - START MARKER (Small blue cylinder at start)        -->
    <!-- ================================================================= -->
    <model name="blue_start_marker">
      <static>true</static>
      <pose>0 4 0 0 0 0</pose>
      <link name="link">
        <visual name="base">
          <pose>0 0 0.02 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.2</radius>
              <length>0.04</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.0 0.0 0.5 1</ambient>
            <diffuse>0.1 0.1 0.7 1</diffuse>
          </material>
        </visual>
        <visual name="ring">
          <pose>0 0 0.05 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.25</radius>
              <length>0.02</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.2 0.2 0.8 1</ambient>
            <diffuse>0.3 0.3 0.9 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- ================================================================= -->
    <!-- ROBOT 1: BLUE - GOAL MARKER (Tall blue pole at goal)               -->
    <!-- ================================================================= -->
    <model name="blue_goal_marker">
      <static>true</static>
      <pose>0 -4 0 0 0 0</pose>
      <link name="pole">
        <visual name="base">
          <pose>0 0 0.02 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.15</radius>
              <length>0.04</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.1 0.1 0.6 1</ambient>
            <diffuse>0.2 0.2 0.8 1</diffuse>
          </material>
        </visual>
        <visual name="pole">
          <pose>0 0 0.5 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>1.0</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.1 0.1 0.8 1</ambient>
            <diffuse>0.2 0.2 0.9 1</diffuse>
            <specular>0.5 0.5 0.5 1</specular>
          </material>
        </visual>
        <visual name="top_sphere">
          <pose>0 0 1.05 0 0 0</pose>
          <geometry>
            <sphere><radius>0.1</radius></sphere>
          </geometry>
          <material>
            <ambient>0.2 0.2 1.0 1</ambient>
            <diffuse>0.3 0.3 1.0 1</diffuse>
            <specular>0.8 0.8 0.8 1</specular>
          </material>
        </visual>
      </link>
    </model>

    <!-- ================================================================= -->
    <!-- ROBOT 2: GREEN - START MARKER (Small green cylinder at start)      -->
    <!-- ================================================================= -->
    <model name="green_start_marker">
      <static>true</static>
      <pose>-4 0 0 0 0 0</pose>
      <link name="link">
        <visual name="base">
          <pose>0 0 0.02 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.2</radius>
              <length>0.04</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.0 0.5 0.0 1</ambient>
            <diffuse>0.1 0.7 0.1 1</diffuse>
          </material>
        </visual>
        <visual name="ring">
          <pose>0 0 0.05 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.25</radius>
              <length>0.02</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.2 0.8 0.2 1</ambient>
            <diffuse>0.3 0.9 0.3 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- ================================================================= -->
    <!-- ROBOT 2: GREEN - GOAL MARKER (Tall green pole at goal)             -->
    <!-- ================================================================= -->
    <model name="green_goal_marker">
      <static>true</static>
      <pose>4 0 0 0 0 0</pose>
      <link name="pole">
        <visual name="base">
          <pose>0 0 0.02 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.15</radius>
              <length>0.04</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.1 0.6 0.1 1</ambient>
            <diffuse>0.2 0.8 0.2 1</diffuse>
          </material>
        </visual>
        <visual name="pole">
          <pose>0 0 0.5 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>1.0</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.1 0.8 0.1 1</ambient>
            <diffuse>0.2 0.9 0.2 1</diffuse>
            <specular>0.5 0.5 0.5 1</specular>
          </material>
        </visual>
        <visual name="top_sphere">
          <pose>0 0 1.05 0 0 0</pose>
          <geometry>
            <sphere><radius>0.1</radius></sphere>
          </geometry>
          <material>
            <ambient>0.2 1.0 0.2 1</ambient>
            <diffuse>0.3 1.0 0.3 1</diffuse>
            <specular>0.8 0.8 0.8 1</specular>
          </material>
        </visual>
      </link>
    </model>

    <!-- ================================================================= -->
    <!--                                                                    -->
    <!--              ROBOT 1: BLUE (Starts at Top, Goes Down)              -->
    <!--                                                                    -->
    <!-- ================================================================= -->
    <!--
    Blue robot starts at (0, 4) and travels SOUTH to (0, -4).
    This is a straight vertical path through the center of the warehouse.
    The path crosses the green robot's path at the origin.
    -->
    
    <model name="turtlebot1">
      <pose>0 4 0.1 0 0 -1.5708</pose>  <!-- Facing SOUTH (negative Y) -->
      
      <!-- Chassis - BLUE -->
      <link name="chassis">
        <pose>0 0 0.05 0 0 0</pose>
        <inertial>
          <mass>5.0</mass>
          <inertia>
            <ixx>0.05</ixx><ixy>0</ixy><ixz>0</ixz>
            <iyy>0.1</iyy><iyz>0</iyz><izz>0.1</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry><box><size>0.5 0.4 0.15</size></box></geometry>
        </collision>
        <visual name="visual">
          <geometry><box><size>0.5 0.4 0.15</size></box></geometry>
          <material>
            <ambient>0.1 0.1 0.8 1</ambient>
            <diffuse>0.2 0.2 0.9 1</diffuse>
            <specular>0.3 0.3 0.3 1</specular>
          </material>
        </visual>
      </link>
      
      <!-- Left Wheel -->
      <link name="left_wheel">
        <pose>-0.1 0.225 0.05 -1.5707 0 0</pose>
        <inertial>
          <mass>0.5</mass>
          <inertia>
            <ixx>0.001</ixx><ixy>0</ixy><ixz>0</ixz>
            <iyy>0.001</iyy><iyz>0</iyz><izz>0.002</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry><cylinder><radius>0.1</radius><length>0.05</length></cylinder></geometry>
          <surface><friction><ode><mu>1.0</mu><mu2>1.0</mu2></ode></friction></surface>
        </collision>
        <visual name="visual">
          <geometry><cylinder><radius>0.1</radius><length>0.05</length></cylinder></geometry>
          <material><ambient>0.1 0.1 0.1 1</ambient><diffuse>0.2 0.2 0.2 1</diffuse></material>
        </visual>
      </link>
      
      <!-- Right Wheel -->
      <link name="right_wheel">
        <pose>-0.1 -0.225 0.05 -1.5707 0 0</pose>
        <inertial>
          <mass>0.5</mass>
          <inertia>
            <ixx>0.001</ixx><ixy>0</ixy><ixz>0</ixz>
            <iyy>0.001</iyy><iyz>0</iyz><izz>0.002</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry><cylinder><radius>0.1</radius><length>0.05</length></cylinder></geometry>
          <surface><friction><ode><mu>1.0</mu><mu2>1.0</mu2></ode></friction></surface>
        </collision>
        <visual name="visual">
          <geometry><cylinder><radius>0.1</radius><length>0.05</length></cylinder></geometry>
          <material><ambient>0.1 0.1 0.1 1</ambient><diffuse>0.2 0.2 0.2 1</diffuse></material>
        </visual>
      </link>
      
      <!-- Caster -->
      <link name="caster">
        <pose>0.2 0 0.025 0 0 0</pose>
        <inertial>
          <mass>0.2</mass>
          <inertia>
            <ixx>0.00004</ixx><ixy>0</ixy><ixz>0</ixz>
            <iyy>0.00004</iyy><iyz>0</iyz><izz>0.00004</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry><sphere><radius>0.025</radius></sphere></geometry>
          <surface><friction><ode><mu>0.01</mu><mu2>0.01</mu2></ode></friction></surface>
        </collision>
        <visual name="visual">
          <geometry><sphere><radius>0.025</radius></sphere></geometry>
          <material><ambient>0.3 0.3 0.3 1</ambient><diffuse>0.4 0.4 0.4 1</diffuse></material>
        </visual>
      </link>
      
      <!-- Joints -->
      <joint name="left_wheel_joint" type="revolute">
        <parent>chassis</parent>
        <child>left_wheel</child>
        <axis><xyz>0 0 1</xyz><limit><lower>-1.79769e+308</lower><upper>1.79769e+308</upper></limit></axis>
      </joint>
      
      <joint name="right_wheel_joint" type="revolute">
        <parent>chassis</parent>
        <child>right_wheel</child>
        <axis><xyz>0 0 1</xyz><limit><lower>-1.79769e+308</lower><upper>1.79769e+308</upper></limit></axis>
      </joint>
      
      <joint name="caster_joint" type="ball">
        <parent>chassis</parent>
        <child>caster</child>
      </joint>
      
      <!-- Differential Drive Plugin -->
      <plugin filename="gz-sim-diff-drive-system" name="gz::sim::systems::DiffDrive">
        <left_joint>left_wheel_joint</left_joint>
        <right_joint>right_wheel_joint</right_joint>
        <wheel_separation>0.45</wheel_separation>
        <wheel_radius>0.1</wheel_radius>
        <max_linear_acceleration>2.0</max_linear_acceleration>
        <max_angular_acceleration>3.0</max_angular_acceleration>
        <topic>/model/turtlebot1/cmd_vel</topic>
        <odom_topic>/model/turtlebot1/odometry</odom_topic>
        <tf_topic>/model/turtlebot1/tf</tf_topic>
        <frame_id>odom</frame_id>
        <child_frame_id>turtlebot1/chassis</child_frame_id>
        <odom_publish_frequency>50</odom_publish_frequency>
      </plugin>
      
    </model>

    <!-- ================================================================= -->
    <!--                                                                    -->
    <!--            ROBOT 2: GREEN (Starts at Left, Goes Right)             -->
    <!--                                                                    -->
    <!-- ================================================================= -->
    <!--
    Green robot starts at (-4, 0) and travels EAST to (4, 0).
    This is a straight horizontal path through the center of the warehouse.
    The path crosses the blue robot's path at the origin.
    -->
    
    <model name="turtlebot2">
      <pose>-4 0 0.1 0 0 0</pose>  <!-- Facing EAST (positive X) -->
      
      <!-- Chassis - GREEN -->
      <link name="chassis">
        <pose>0 0 0.05 0 0 0</pose>
        <inertial>
          <mass>5.0</mass>
          <inertia>
            <ixx>0.05</ixx><ixy>0</ixy><ixz>0</ixz>
            <iyy>0.1</iyy><iyz>0</iyz><izz>0.1</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry><box><size>0.5 0.4 0.15</size></box></geometry>
        </collision>
        <visual name="visual">
          <geometry><box><size>0.5 0.4 0.15</size></box></geometry>
          <material>
            <ambient>0.1 0.8 0.1 1</ambient>
            <diffuse>0.2 0.9 0.2 1</diffuse>
            <specular>0.3 0.3 0.3 1</specular>
          </material>
        </visual>
      </link>
      
      <!-- Left Wheel -->
      <link name="left_wheel">
        <pose>-0.1 0.225 0.05 -1.5707 0 0</pose>
        <inertial>
          <mass>0.5</mass>
          <inertia>
            <ixx>0.001</ixx><ixy>0</ixy><ixz>0</ixz>
            <iyy>0.001</iyy><iyz>0</iyz><izz>0.002</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry><cylinder><radius>0.1</radius><length>0.05</length></cylinder></geometry>
          <surface><friction><ode><mu>1.0</mu><mu2>1.0</mu2></ode></friction></surface>
        </collision>
        <visual name="visual">
          <geometry><cylinder><radius>0.1</radius><length>0.05</length></cylinder></geometry>
          <material><ambient>0.1 0.1 0.1 1</ambient><diffuse>0.2 0.2 0.2 1</diffuse></material>
        </visual>
      </link>
      
      <!-- Right Wheel -->
      <link name="right_wheel">
        <pose>-0.1 -0.225 0.05 -1.5707 0 0</pose>
        <inertial>
          <mass>0.5</mass>
          <inertia>
            <ixx>0.001</ixx><ixy>0</ixy><ixz>0</ixz>
            <iyy>0.001</iyy><iyz>0</iyz><izz>0.002</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry><cylinder><radius>0.1</radius><length>0.05</length></cylinder></geometry>
          <surface><friction><ode><mu>1.0</mu><mu2>1.0</mu2></ode></friction></surface>
        </collision>
        <visual name="visual">
          <geometry><cylinder><radius>0.1</radius><length>0.05</length></cylinder></geometry>
          <material><ambient>0.1 0.1 0.1 1</ambient><diffuse>0.2 0.2 0.2 1</diffuse></material>
        </visual>
      </link>
      
      <!-- Caster -->
      <link name="caster">
        <pose>0.2 0 0.025 0 0 0</pose>
        <inertial>
